    "globals": "^15.15.0",
    "jsdom": "^25.0.1",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.26.1",
    "vite": "^5.4.14",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "keywords": [
